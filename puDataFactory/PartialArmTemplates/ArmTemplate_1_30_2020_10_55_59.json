					{
						"pipelineReference": {
							"referenceName": "Communication",
							"type": "PipelineReference"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 1,
						"startTime": "2019-03-07T17:45:00.000Z",
						"timeZone": "UTC",
