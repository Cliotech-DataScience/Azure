					"folder": {
						"type": "String"
					}
				},
				"folder": {
					"name": "Platform Usage"
				},
				"type": "FileShare",
				"typeProperties": {
					"compression": {
						"type": "ZipDeflate",
						"level": "Optimal"
					},
					"fileName": {
						"value": "@dataset().file_name",
						"type": "Expression"
					},
					"folderPath": {
						"value": "@dataset().folder",
