{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "puDataFactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/DWH_Dim_Users')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "copy_to_blob",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "SqlSource",
								"sqlReaderQuery": {
									"value": "select * from  dwh.azure.Dim_Users\n ",
									"type": "Expression"
								}
							},
							"sink": {
								"type": "BlobSink"
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0
						},
						"inputs": [
							{
								"referenceName": "Customers",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "blob_Customers",
								"type": "DatasetReference",
								"parameters": {
									"folder_name": "Customers"
								}
							}
						]
					},
					{
						"name": "Load_Users",
						"type": "DatabricksNotebook",
						"dependsOn": [
							{
								"activity": "copy_to_blob",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"notebookPath": "/Load Data/Dimensions/Customers"
						},
						"linkedServiceName": {
							"referenceName": "Analysis",
							"type": "LinkedServiceReference"
						}
					}
				],
				"folder": {
					"name": "Data_from_SQL"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/dwh_Dim_Users_blob')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "DataloadStore",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"folder_name": {
						"type": "String",
						"defaultValue": "Dim_AccountContacts"
					}
				},
				"folder": {
					"name": "DWH"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "User_ID",
						"type": "Int16"
					},
					{
						"name": "User_Name",
						"type": "String"
					},
					{
						"name": "User_Status",
						"type": "String"
					},
					{
						"name": "Manager_Name",
						"type": "String"
					},
					{
						"name": "User_Type",
						"type": "String"
					},
					{
						"name": "User_Category",
						"type": "String"
					},
					{
						"name": "Email",
						"type": "String"
					},
					{
						"name": "Last_Login_Date",
						"type": "DateTime"
					},
					{
						"name": "Salesman_Serial",
						"type": "Int32"
					},
					{
						"name": "Exentsion",
						"type": "String"
					},
					{
						"name": "Logon_Name",
						"type": "String"
					},
					{
						"name": "Team",
						"type": "String"
					},
					{
						"name": "AD_Name",
						"type": "String"
					},
					{
						"name": "AD_Display_Name",
						"type": "String"
					},
					{
						"name": "AD_OU_Path",
						"type": "String"
					},
					{
						"name": "AD_Extension",
						"type": "String"
					},
					{
						"name": "AD_Department",
						"type": "String"
					},
					{
						"name": "Language",
						"type": "String"
					},
					{
						"name": "Main_Broker",
						"type": "String"
					},
					{
						"name": "Main_Parent_Group",
						"type": "String"
					},
					{
						"name": "details",
						"type": "String"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"fileName": {
						"value": "@CONCAT('Users_', pipeline().RunId, '.csv')",
						"type": "Expression"
					},
					"folderPath": "rawdata/Users"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/dwh_Dim_Users_src')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "alldbdwh",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "DWH"
				},
				"type": "SqlServerTable",
				"structure": [
					{
						"name": "User_ID",
						"type": "Int16"
					},
					{
						"name": "User_Name",
						"type": "String"
					},
					{
						"name": "User_Status",
						"type": "String"
					},
					{
						"name": "Manager_Name",
						"type": "String"
					},
					{
						"name": "User_Type",
						"type": "String"
					},
					{
						"name": "User_Category",
						"type": "String"
					},
					{
						"name": "Email",
						"type": "String"
					},
					{
						"name": "Last_Login_Date",
						"type": "DateTime"
					},
					{
						"name": "Salesman_Serial",
						"type": "Int32"
					},
					{
						"name": "Extension",
						"type": "String"
					},
					{
						"name": "Logon_Name",
						"type": "String"
					},
					{
						"name": "Team",
						"type": "String"
					},
					{
						"name": "AD_Name",
						"type": "String"
					},
					{
						"name": "AD_Display_Name",
						"type": "String"
					},
					{
						"name": "AD_OU_Path",
						"type": "String"
					},
					{
						"name": "AD_Extension",
						"type": "String"
					},
					{
						"name": "AD_Department",
						"type": "String"
					},
					{
						"name": "Language",
						"type": "String"
					},
					{
						"name": "Main_Broker",
						"type": "String"
					},
					{
						"name": "Main_Parent_Group",
						"type": "String"
					},
					{
						"name": "details",
						"type": "String"
					}
				],
				"typeProperties": {
					"tableName": "dwh.azure.Dim_Users"
				}
			}
		}
	]
}