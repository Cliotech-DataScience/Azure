{
	"name": "sync_customers_clusters",
	"properties": {
		"activities": [
			{
				"name": "customers_clusters",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "Customers_Models_Data"
					},
					{
						"name": "Destination",
						"value": "acc_tab"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlSource",
						"sqlReaderQuery": "SELECT TOP 1  AccountNumber  FROM Customers_Models_Data"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"sqlWriterStoredProcedureName": "[dbo].[testazuresp]",
						"sqlWriterTableType": "AccType"
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"AccountNumber": "Acc"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Customers_Models_Data",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "alldbsp",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}