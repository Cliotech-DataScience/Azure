{
	"name": "Sync_customers_clustering",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"AccountNumber": "AccountNumber",
							"cluster_type": "cluster_type",
							"update_date": "update_date"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "blob_customers_clustering",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "blob_customers_clustering",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Data_to_SQL"
		}
	}
}