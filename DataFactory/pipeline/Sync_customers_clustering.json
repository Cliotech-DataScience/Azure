{
	"name": "Sync_customers_clustering",
	"properties": {
		"activities": [
			{
				"name": "Copy_csv_to_STG",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "LK_file_name",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"AccountNumber": "AccountNumber",
							"cluster_type": "cluster_type",
							"update_date": "update_date"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "blob_customers_clustering",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "STG_mrr_customers_clustering",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "delete_all_file",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "Copy_csv_to_STG",
						"dependencyConditions": [
							"Skipped"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataset": {
						"referenceName": "blob_customers_clustering_json_file_list",
						"type": "DatasetReference"
					},
					"recursive": true,
					"enableLogging": false
				}
			},
			{
				"name": "LK_file_name",
				"type": "Lookup",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"dataset": {
						"referenceName": "blob_customers_clustering_json_file_list",
						"type": "DatasetReference"
					}
				}
			}
		],
		"folder": {
			"name": "Data_to_SQL"
		}
	}
}