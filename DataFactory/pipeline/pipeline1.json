						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "accountnumber",
									"type": "Int64"
								},
								"sink": {
									"name": "accountnumber",
									"type": "Int64"
								}
							},
							{
								"source": {
									"name": "Event_date",
									"type": "DateTime"
								},
								"sink": {
									"name": "Event_date",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"name": "event_name",
									"type": "string"
								},
								"sink": {
									"name": "event_name",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureDatabricksDeltaLakeDataset1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "all_db_pr_SQL_SERVER",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Platfrom_Usage/export_data"
		},
		"annotations": []
	}
