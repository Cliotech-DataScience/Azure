{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "bi-db/dim_serials/*.parquet"
					},
					{
						"name": "Destination",
						"value": "acc_tab"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"sqlWriterStoredProcedureName": "[dbo].[testazuresp]",
						"sqlWriterTableType": "AccType"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"Serial": "Acc"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "BI_Serials_blob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "alldbsp",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}