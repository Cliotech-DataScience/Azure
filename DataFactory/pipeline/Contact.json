									"type": "DateTime"
								},
								"sink": {
									"name": "Modifiedon",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"name": "new_scmmcontactid",
									"type": "String"
								},
								"sink": {
									"name": "Scmm_Contact_Id",
