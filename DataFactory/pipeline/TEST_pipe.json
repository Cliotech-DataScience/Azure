{
	"name": "TEST_pipe",
	"properties": {
		"activities": [
			{
				"name": "email crm to sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CommonDataServiceForAppsSource",
						"additionalColumns": [
							{
								"name": "ETL_update_date",
								"value": {
									"value": "@utcnow()",
									"type": "Expression"
								}
							}
						],
						"query": "<fetch>  \n   <entity name='contact'> \n     <all-attributes/>    \n<filter type=\"and\">\n<condition attribute='modifiedon' operator=\"olderthan-x-days\" value=\"1\" />\n</filter>    \n   </entity>   \n</fetch>"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Dataverse_AsyncOperation",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "all_db_pr_SQL_SERVER",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "CRM"
		},
		"annotations": []
	}
}