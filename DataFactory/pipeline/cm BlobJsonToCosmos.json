				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dwh-qa/sqldata/campaingscsv.json"
					},
					{
						"name": "Destination",
						"value": "campaigns"
					}
				],
				"typeProperties": {
