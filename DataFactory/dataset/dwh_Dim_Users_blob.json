{
	"name": "dwh_Dim_Users_blob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "DataloadStore",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder_name": {
				"type": "String",
				"defaultValue": "Dim_AccountContacts"
			}
		},
		"folder": {
			"name": "DWH"
		},
		"type": "AzureBlob",
		"structure": [
			{
				"name": "User_ID",
				"type": "Int16"
			},
			{
				"name": "details",
				"type": "String"
			}
		],
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": true
			},
			"fileName": {
				"value": "@CONCAT('Users_', pipeline().RunId, '.csv')",
				"type": "Expression"
			},
			"folderPath": "rawdata/Users"
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}