{
	"name": "blob_customers_clustering_folder",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder_name": {
				"type": "String"
			}
		},
		"folder": {
			"name": "sync"
		},
		"type": "AzureBlob",
		"structure": [
			{
				"name": "AccountNumber",
				"type": "String"
			},
			{
				"name": "cluster_type",
				"type": "String"
			},
			{
				"name": "update_date",
				"type": "String"
			}
		],
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"encodingName": null,
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": true
			},
			"fileName": {
				"value": "*.*",
				"type": "Expression"
			},
			"folderPath": {
				"value": "@concat('rawdata/export_file/customers_clustering/',dataset().folder_name)",
				"type": "Expression"
			}
		}
	}
}