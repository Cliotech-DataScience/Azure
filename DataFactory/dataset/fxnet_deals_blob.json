{
	"name": "fxnet_deals_blob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "DataloadStore",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"structure": [
			{
				"name": "TransactionNumber",
				"type": "String"
			},
			{
				"name": "Trans_Details",
				"type": "String"
			},
			{
				"name": "ExecutionDay",
				"type": "String"
			}
		],
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": "\t",
				"rowDelimiter": "",
				"quoteChar": "\"",
				"nullValue": "\\N",
				"encodingName": null,
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"compression": {
				"type": "BZip2"
			},
			"fileName": {
				"value": "@CONCAT('FXNET_Deals_', pipeline().TriggerTime, '.csv.bz2')",
				"type": "Expression"
			},
			"folderPath": "@concat('rawdata/FXNET_Deals/Received=',utcnow())"
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}